{
  "manifest_version": 3,
  "name": "Focus Mode",
  "description": "Enable focus mode on Chrome's official Extensions and Chrome Web Store documentation.",
  "version": "1.0",
  "icons": {
    "128": "logo192.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open",
    "default_icon": {
      "128": "logo192.png"
    }
  },
  "permissions": [
    "scripting"
  ]
}